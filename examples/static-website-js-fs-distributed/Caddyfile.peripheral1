{
	cert_issuer internal
	admin off
	broker {
		port 4223
		jetstream {
			domain peripheral1
		}
		auth_account AUTH
		account websites {
			leafnode_connect nats://peripheral1@localhost:7422
			jetstream
		}
	}
}

static.local.quara-dev.com {
    rewrite / /index.html
    file_server {
        fs jetstream {
			account websites
            store static-app
            sync_dir ./cache/peripheral1/static-app
			client {
				domain central
			}
        }
    }
}
