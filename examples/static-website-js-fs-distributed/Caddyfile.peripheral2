{
	cert_issuer internal
	https_port 444
	broker {
		port 4224
		jetstream {
			domain peripheral2
		}
		auth_account AUTH
		account websites {
			leafnode_connect nats://peripheral2@localhost:7422
			jetstream
		}
	}
}

static.local.quara-dev.com {
    rewrite / /index.html
    file_server {
        fs jetstream {
			account websites
            store static-app
            sync_dir ./cache/peripheral2/static-app
			client {
				domain central
			}
        }
    }
}
