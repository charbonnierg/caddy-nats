{
    "logging": {
        "logs": {
            "default": {
                "level": "DEBUG"
            }
        }
    },
    "apps": {
        "tls": {
            "automation": {
                "policies": [
                    {
                        "subjects": [
                            "localhost"
                        ],
                        "issuers": [
                            {
                                "module": "internal"
                            }
                        ],
                        "disable_ocsp_stapling": true
                    }
                ]
            }
        },
        "nats": {
            "auth_service": {
                "internal_account": "AUTH",
                "handler": {
                    "module": "always_allow"
                }
            },
            "server": {
                "name": "test",
                "tags": {
                    "env": "demo"
                },
                "debug": true,
                "trace": true,
                "http_port": 8222,
                "tls": {
                    "subjects": [
                        "localhost"
                    ]
                },
                "system_account": "SYS",
                "accounts": [
                    {
                        "name": "APP"
                    },
                    {
                        "name": "SYS"
                    }
                ],
                "jetstream": {},
                "mqtt": {},
                "leafnode": {
                    "tls": {
                        "subjects": [
                            "localhost"
                        ]
                    }
                },
                "websocket": {
                    "tls": {
                        "subjects": [
                            "localhost"
                        ]
                    }
                },
                "metrics": {
                    "connz": true,
                    "connz_detailed": true
                }
            }
        },
        "http": {
            "servers": {
                "https": {
                    "listen": [
                        ":80",
                        ":443"
                    ],
                    "routes": [
                        {
                            "match": [
                                {
                                    "host": [
                                        "localhost"
                                    ]
                                }
                            ],
                            "handle": [
                                {
                                    "handler": "metrics"
                                }
                            ],
                            "terminal": true
                        }
                    ],
                    "metrics": {}
                }
            }
        }
    }
}